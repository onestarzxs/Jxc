@using Gms.Domain

@{
    Layout = "~/Views/Shared/EmptyLayout.cshtml";
}

<body>

    <div id="toolbar" style="padding: 5px; height: auto">
        <div>
            姓名：<input name="Name" class="messager-input" style="width: 80px" />
            电话：<input name="Mobile" class="messager-input" style="width: 80px" />
            公司：<input name="Company" class="messager-input" style="width: 80px" />
            <a href="#" id="supplier_search" class="easyui-linkbutton search" iconcls="icon-search">搜索</a>
        </div>
    </div>
     
    <table id="supplier_search_list" url="@Url.Action("List","Supplier")" del="@Url.Action("Delete","Supplier")">
            
    </table>

    
    @*记录选择的项，便于其他页面调用*@
    <div style="display: none;">
        <input id="supplier_selected_id" />
        <input id="supplier_selected_name" />
    </div>
       
        
</body>
<script type="text/javascript" src="@Url.Content("/Scripts/Supplier/Supplier.js")"></script>
<script type="text/javascript">
    $(document).ready(function () {
        Supplier.Select();
    });
    
</script>

@*</html>*@
