

@{
    ViewBag.Title = "加工过程";
    Layout = "~/Views/Shared/EmptyLayout.cshtml";
}

<html>
    <body>
       
        <div id="toolbar" style="padding: 5px; height: auto">
        </div>

        <table id="cureprocess_list" url="@Url.Action("List","CureProcess")" >
            
        </table>
  
        @*记录选择的项，便于其他页面调用*@
        <div style="display: none;">
            <input id="process_selected_id" />
            <input id="process_selected_name" />
            <input id="process_selected_desc" />
        </div>

    </body>
    <script type="text/javascript" src="@Url.Content("/Scripts/CureProcess/CureProcess.js")"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            CureProcess.Init();
        });

    </script>
</html>
